<% if flash[:alert] %>
<%= js add_gritter( flash [:alert], title: "Maxwell Galpin", sticky: false) %>
<% elsif flash[:error] %>
<%= js add_gritter( flash [:error], title: "Maxwell Galpin", sticky: false) %>
<% else %>
<%= js add_gritter( flash[:notice], title: "Maxwell Galpin", sticky: false) %>
<% end %>